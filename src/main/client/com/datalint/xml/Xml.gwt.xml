<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to='xml'>
	<inherits name='com.google.gwt.user.User' />

	<set-property name="user.agent" value="safari" />
	<extend-property name="user.agent" values="gecko1_8" />
	<extend-property name="user.agent" values="ie11" />

	<source path='client' />
	<source path='shared' />
	<super-source path="super" />
</module>
